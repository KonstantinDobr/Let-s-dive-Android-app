<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/signFragment">
    <fragment
        android:id="@+id/signFragment"
        android:name="com.example.letsdive.authorization.ui.sign.SignFragment"
        android:label="SignFragment"
        tools:layout="@layout/fragment_sign" >
        <action
            android:id="@+id/action_signFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.letsdive.MainActivity"
        android:label="MainActivity" />
</navigation>