<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/signFragment">
    <fragment
        android:id="@+id/signFragment"
        android:name="com.example.letsdive.authorization.ui.sign.SignFragment"
        android:label="SignFragment"
        tools:layout="@layout/fragment_sign" >
        <action
            android:id="@+id/action_signFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.letsdive.authorization.ui.main_fragments.ProfileFragment"
        android:label="ProfileFragment" />
</navigation>